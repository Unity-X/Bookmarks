<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="BookmarksWindow.uss" />
    <ui:Button display-tooltip-when-elided="true" name="button" class="item">
        <ui:VisualElement name="icon" style="width: 16px; height: 16px; margin-right: 1px; min-width: 16px; min-height: 16px;" />
        <ui:Label text="Label" display-tooltip-when-elided="true" name="label" style="-unity-text-align: middle-left; padding-right: 0;" />
